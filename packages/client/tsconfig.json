{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@services/*": [
        "src/app/common/services/*"
      ],
      "@guards": [
        "src/app/common/guards"
      ],
      "@interceptors": [
        "src/app/common/interceptors"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@models": [
        "src/app/common/models"
      ]
    }
  }
}